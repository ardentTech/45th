{
  "manifest_version": 2,
  "name": "45th",
  "description": "Glorify the 45th Presidential Administration with their own words.",
  "version": "0.1",
  "content_scripts": [
      {
          "matches": ["*://*/*"],
          "js": ["data.js", "main.js"],
          "run_at": "document_end"
      }
  ],
  "permissions": ["storage"],
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "45th"
  }
}
